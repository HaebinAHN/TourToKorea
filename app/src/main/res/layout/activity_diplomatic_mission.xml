<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".diplomatic_mission">

    <fragment
        android:id="@+id/map_Location"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="415dp"
        android:layout_height="437dp"
        android:layout_alignParentTop="true"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".Map_fragment" />

    <TextView
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/map_Location"
        android:layout_marginTop="14dp"
        android:text="Location Information :"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/Location_adr"
        android:layout_width="363dp"
        android:layout_height="22dp"
        android:layout_below="@+id/editTextTextPersonName2"
        android:layout_marginTop="13dp"
        android:text="@string/diplomatic_mission"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        tools:layout_editor_absoluteX="24dp" />

    <Button
        android:id="@+id/transport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView4"
        android:layout_marginTop="13dp"
        android:text="@string/submit"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_editor_absoluteX="241dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Location_adr"
        android:layout_marginTop="16dp"
        android:text="Look around from diplomatic mission"
        app:layout_constraintTop_toBottomOf="@+id/Location_adr"
        tools:layout_editor_absoluteX="161dp" />

    <Button
        android:id="@+id/Look_around"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="13dp"
        android:text="@string/submit"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_editor_absoluteX="241dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Location_adr"
        android:layout_marginTop="108dp"
        android:text="Public transportation guidance to that location"
        app:layout_constraintTop_toBottomOf="@+id/Look_around"
        tools:layout_editor_absoluteX="107dp" />


</RelativeLayout>