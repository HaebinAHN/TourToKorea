<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DirectionActivity">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="415dp"
        android:layout_height="437dp"
        tools:context=".MapsActivity"

    <EditText
        android:id="@+id/ToText"
        android:layout_width="147dp"
        android:layout_height="44dp"
        android:hint="To"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="581dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="549dp" />

    <EditText
        android:id="@+id/FromText"
        android:layout_width="147dp"
        android:layout_height="44dp"
        android:hint="From"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="498dp" />

    <Button
        android:id="@+id/placeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="685dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Look around from your position"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="650dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="From"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="470dp" />

</androidx.constraintlayout.widget.ConstraintLayout>